<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">

      <div class="row">

        <div class="col-xs-12">
             <div class="header"><h2>Blog Rails</h2></div>
        </div>

      </div>

       <div class="row">

          <div class="col-xs-9">

             <%= yield %>
          </div>

          <div class="col-xs-3 ">
                <div class="aside"> <%= render 'last_article' %></div>
          </div>

      </div>

  </div>

</body>
</html>
